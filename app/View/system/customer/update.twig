        $("#contact_type").on('change', '#select_type', function () {
            addMaskToInput();
        });

        function addMaskToInput() {
            let $select_type = $('#select_type option:selected').val();
            let $contact = $('#contact');
            if ($select_type !== '1') {
                $contact.mask('(00)00000-0000');
            } else {
                $contact.unmask();
            }
        }


        $("#contact_type_existed").on('change', '#select_type_existed', function () {
            addMaskToInputExisted();
        });

        function addMaskToInputExisted() {
            let $contact_existed = $('#contact_existed');
            let $select_type_existed = $('#select_type_existed option:selected').val();

            if ($select_type_existed !== '1') {
                $contact_existed.mask('(00)00000-0000');
            } else {
                $contact_existed.unmask();
            }
        }


        $(function () {
            $(':input[name=document]').mask(docMask, docOptions);
        });

        let docMask = function (val) {
                return val.replace(/\D/g, '').length <= 11 ? '000.000.000-009' : '00.000.000/0000-00';
            },
            docOptions = {
                onKeyPress: function (val, e, field, options) {
                    field.mask(docMask.apply({}, arguments), options);
                }
            };

        $(function () {
            $(':input[name=document]').mask(docMask, docOptions);
        });


        $(document).ready(function () {
            $('.select2-test').select2();

            $('.fieldset_bank').metalClone({
                btnClone: '.btn_fieldset_bank',
                btnRemoveClass: 'btn btn-danger btn-sm',
                confirmMessageText: 'Deseja mesmo apagar?',
                onComplete: function (e, o, c) {
                    let cc = 0;
                    $('.fieldset_bank').each(function (i) {
                        cc = eval(cc + 1);
                    })
                    $(c[0]).find(':input').each(function (ee) {
                        if ($(this).attr('name')) $(this).attr('name', $(this).attr('name').replace(0, cc));
                    })
                }
            });

            $('.fieldset_contact').metalClone({
                btnClone: '.btn_fieldset_contact',
                btnRemoveClass: 'btn btn-danger btn-sm',
                confirmMessageText: 'Deseja mesmo apagar?',
                onComplete: function (e, o, c) {
                    let cc = 0;
                    $('.fieldset_contact').each(function (i) {
                        cc = eval(cc + 1);
                    })
                    $(c[0]).find(':input').each(function (ee) {
                        if ($(this).attr('name')) $(this).attr('name', $(this).attr('name').replace(0, cc));
                    })
                }
            });

            $('.fieldset_address').metalClone({
                btnClone: '.btn_fieldset_address',
                btnRemoveClass: 'btn btn-danger btn-sm',
                confirmMessageText: 'Deseja mesmo apagar?',
                onComplete: function (e, o, c) {
                    let cc = 0;
                    $('.fieldset_address').each(function (i) {
                        cc = eval(cc + 1);
                    })
                    $(c[0]).find(':input').each(function (ee) {
                        if ($(this).attr('name')) $(this).attr('name', $(this).attr('name').replace(0, cc));
                    })
                }
            });

            const footerTemplate = '<div class="file-thumbnail-footer" style ="height:94px">\n' +
                '  <input id="{document_id}" class="kv-input kv-new form-control input-sm form-control-sm text-center {TAG_CSS_NEW}" ' +
                'value="{caption}" name="title[{document_id}]" placeholder="Enter caption...">\n' +
                '   <div class="small" style="margin:15px 0 2px 0">{size}</div> {progress}\n{indicator}\n{actions}\n' +
                '</div>';


            $("#kv-explorer").fileinput({
                initialPreview: {{ preview|raw }},
                initialPreviewConfig: {{ previewConfig|raw }},
                layoutTemplates: {footer: footerTemplate},
                allowedPreviewTypes: ['image', 'pdf', 'video'],
                'theme': 'fas',
                'uploadUrl': "{{ url_for('upload.customer', {hash: customer.hash} ) }}",
                'deleteUrl': "{{ url_for('file.delete', {type: 'customer'}) }}",
                previewFileIcon: '<i class="fas fa-file"></i>',
                previewFileIconSettings: {
                    'docx': '<i class="fas fa-file-word text-primary"></i>'
                },
                initialPreviewThumbTags: {{ initialPreviewThumbTags|raw }},
                language: 'pt-BR',
                overwriteInitial: false,
                initialPreviewAsData: true,

            });

            $(".button_delete_bank").click(function () {
                const hash = $('#' + $(this).data('hash'));
                Swal.fire({
                    title: 'Deseja mesmo excluir?',
                    text: "Não poderá retornar a execução.",
                    type: 'warning',
                    showCancelButton: true,
                    confirmButtonColor: '#3085d6',
                    cancelButtonColor: '#d33',
                    confirmButtonText: 'Sim, quero excluir!',
                    cancelButtonText: 'Cancelar',
                    showLoaderOnConfirm: true,
                    preConfirm: () => {
                        return fetch($.ajax({
                            url: '/system/customer/bank/' + hash[0].id,
                            method: 'DELETE',
                            success: function (data) {
                                if (data) {
                                    alert('Banco excluído')
                                }
                            }
                        })).then(response => {
                            hash.remove();
                            location.reload(false)
                        }).catch(error => {
                            location.reload(false)
                        })

                    }
                });
            });

            $(".button_delete_contact").click(function () {
