{% extends "/template/template.twig" %}

{% block title %} - Dashboard{% endblock %}


{% block content %}

    <ul class="nav nav-tabs nav-justified" id="myTab" role="tablist">
        <li class="nav-item">
            <a class="nav-link active" id="home-tab" data-toggle="tab" href="#details" role="tab" aria-controls="home"
               aria-selected="true">Detalhes do Cadastro</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" id="profile-tab" data-toggle="tab" href="#proposals" role="tab" aria-controls="profile"
               aria-selected="false">Propostas</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" id="contact-tab" data-toggle="tab" href="#policies" role="tab" aria-controls="contact"
               aria-selected="false">Apólices</a>
        </li>
        <li class="nav-item">
            <a class="nav-link" id="contact-tab" data-toggle="tab" href="#claims" role="tab" aria-controls="contact"
               aria-selected="false">Sinistros</a>
        </li>
    </ul>


    <div class="tab-content bd bd-gray-300 bd-t-0 pd-20" id="myTabContent">
        <div class="tab-pane fade show active" id="details" role="tabpanel" aria-labelledby="home-tab">
            <form id="form-update" action="/system/customer/{{ customer.hash }}" method="post"
                  enctype="multipart/form-data">

                <div class="d-sm-flex align-items-center justify-content-between mg-b-20 mg-lg-b-25 mg-xl-b-30">
                    <div>
                        <nav aria-label="breadcrumb">
                            <ol class="breadcrumb breadcrumb-style1 mg-b-10">
                                <li class="breadcrumb-item"><a href="#">CAPUTO</a></li>
                                <li class="breadcrumb-item"><a href="#">Clientes</a></li>
                                <li class="breadcrumb-item"><a href="#">Formulário</a></li>
                            </ol>
                        </nav>
                        <h4 class="mg-b-0 tx-spacing--1"> Formulário do Cliente</h4>
                    </div>
                    <div class="">
                        <button hidden class="btn btn-lg btn-primary">Salvar Cliente</button>
                    </div>

                    <div id="edit" class="">
                        <button id="removeDisabled" class="btn btn-lg btn-primary">Editar</button>
                    </div>
                </div>

                <fieldset class="form-fieldset">
                    <legend>Informações Gerais</legend>
                    <div class="form-row">
                        <div class="form-group col-md-5">
                            <label>Nome/Razão Social</label>
                            <input readonly value="{{ customer.getFirstName }}" type="text" class="form-control"
                                   name="first_name">
                        </div>
                        <div class="form-group col-md-5">
                            <label>Sobrenome/Nome Fantasia</label>
                            <input readonly value="{{ customer.getLastName }}" type="text" class="form-control"
                                   name="last_name">
                        </div>
                        <div class="form-group col-md-2">
                            <label>Status</label>
                            <select disabled class="custom-select" name="status">
                                <option {% if customer.getStatus == 0 %} selected="selected" {% endif %} value=0>
                                    Deletado
                                </option>
                                <option {% if customer.getStatus == 1 %} selected="selected" {% endif %} value=1>
                                    Ativo
                                </option>
                            </select>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-3">
                            <label>CPF/CNPJ</label>
                            <input readonly value="{{ customer.getDocument }}" type="document"
                                   class="form-control cpfcnpj" name="document">
                        </div>
                        <div class="form-group col-md-3">
                            <label>Data Aniversário</label>
                            <input readonly value="{{ customer.getBirthday|date("d/m/Y") }}" type="text"
                                   class="form-control"
                                   data-mask="00/00/0000" name="birthday">
                        </div>
                        <div class="form-group col-md-3">
                            <label>Sexo</label>
                            <select disabled class="custom-select" name="gender">
                                <option {% if customer.getGender == 'F' %} selected="selected" {% endif %} value="F">
                                    Feminino
                                </option>
                                <option {% if customer.getGender == 'M' %} selected="selected" {% endif %} value="M">
                                    Masculino
                                </option>
                            </select>
                        </div>
                        <div class="form-group col-md-3">
                            <label>Estado Civil</label>
                            <select disabled class="custom-select" name="civil_status">
                                <option {% if customer.getCivilStatus == 1 %} selected="selected" {% endif %} value="1">
                                    Solteiro
                                </option>
                                <option {% if customer.getCivilStatus == 2 %} selected="selected" {% endif %} value="2">
                                    Casado
                                </option>
                                <option {% if customer.getCivilStatus == 3 %} selected="selected" {% endif %} value="3">
                                    Separado
                                </option>
                                <option {% if customer.getCivilStatus == 4 %} selected="selected" {% endif %} value="4">
                                    Divorciado
                                </option>
                                <option {% if customer.getCivilStatus == 5 %} selected="selected" {% endif %} value="5">
                                    Viúvo
                                </option>
                            </select>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-3">
                            <label>CNH</label>
                            <input readonly value="{{ customer.getDocumentCnh }}" type="text" class="form-control"
                                   name="document_cnh"
                                   maxlength="60">
                        </div>
                        <div class="form-group col-md-3">
                            <label>Primeira Habilitação</label>
                            <input readonly value="{{ customer.getFirstCnhDate|date("d/m/Y") }}" type="text"
                                   data-mask="00/00/0000"
                                   class="form-control" name="first_cnh_date">
                        </div>
                        <div class="form-group col-md-3">
                            <label>Data Emissão</label>
                            <input readonly value="{{ customer.getEmissionCnhDate|date("d/m/Y") }}" type="text"
                                   data-mask="00/00/0000"
                                   class="form-control" name="emission_cnh_date">
                        </div>
                        <div class="form-group col-md-3">
                            <label>Data Vencimento</label>
                            <input readonly value="{{ customer.getCnhDueDate|date("d/m/Y") }}" type="text"
                                   data-mask="00/00/0000"
                                   class="form-control" name="cnh_due_date">
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-4">
                            <label>RG</label>
                            <input readonly type="text" class="form-control" required name="document_rg"
                                   value="{{ customer.getDocumentRg }}">
                        </div>
                        <div class="form-group col-md-4">
                            <label>Orgão Expedidor</label>
                            <input readonly type="text" class="form-control" required name="document_rg_origin"
                                   value="{{ customer.getDocumentRgOrigin }}">
                        </div>
                        <div class="form-group col-md-4">
                            <label>Data de Expedição</label>
                            <input readonly
                                   value="{{ customer.getDocumentRgDate ? customer.getDocumentRgDate|date("d/m/Y") : '' }}"
                                   type="text" data-mask="00/00/0000"
                                   class="form-control" name="document_rg_date">
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-6">
                            <label>Profissão</label>
                            <input readonly value="{{ customer.getProfession }}" type="text" class="form-control"
                                   name="profession">
                        </div>
                    </div>

                    <div class="form-row">
                        <div class="form-group col-md-12">
                            <label>Observações</label>
                            <textarea readonly type="text" class="form-control" rows="3"
                                      name="observation">{{ customer.getObservation }}</textarea>
                        </div>
                    </div>
                </fieldset>

